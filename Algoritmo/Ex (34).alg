algoritmo "Fatorial"
var numero, contador, fatorial: inteiro
inicio
      escreva("Digite um numero: ")
      leia(numero)
      fatorial <- 1
      contador <- 2
      enquanto (contador <= numero)  faca
           fatorial <- fatorial * contador
           contador <- contador + 1
      fimenquanto
      escreva("O fatorial de ", numero, " e ", fatorial)
fimalgoritmo
